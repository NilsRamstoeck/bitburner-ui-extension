{
  "name": "bitburner-ui-extension",
  "version": "0.0.1",
  "main": "dist/compiled/electron/main.js",
  "scripts": {
    "dev": "concurrently -k -s first \"vite --port 3000\" \"npm run tsc:watch:electron\" \"npm run electron\"",
    "build": "npm run electron:builder",
    "compile": "vite build && npm run tsc:electron",
    "electron": "wait-on tcp:localhost:3000 && cross-env IS_DEV=true electron .",
    "tsc:electron": "cd src/electron && tsc",
    "tsc:watch:electron": "cd src/electron && tsc -w --preserveWatchOutput true",
    "electron:builder": "electron-builder",
    "pretsc:electron": "mkdirp dist/compiled/ -p && cp -rT ./i18n dist/compiled/i18n/",
    "prebuild": "npm run compile",
    "predev": "npm run tsc:electron"
  },
  "build": {
    "files": [
      "./dist/compiled/**/*"
    ]
  },
  "dependencies": {
    "d3": "^7.8.2",
    "i18next": "^21.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@types/websocket": "^1.0.5",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.4",
    "electron-builder": "^24.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "wait-on": "^7.0.1"
  },
  "author": "Nils Ramst√∂ck",
  "description": "A Template for Electron Apps with React",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NilsRamstoeck/bitburner-ui-extension.git"
  },
  "bugs": {
    "url": "https://github.com/NilsRamstoeck/bitburner-ui-extension/issues"
  },
  "homepage": "https://github.com/NilsRamstoeck/bitburner-ui-extension#readme"
}
